@*@model TCCCMS.Models.Manual*@

<!-- Modal -->
<div class="modal fade" id="formPreviewModal" role="dialog">
    <div class="modal-dialog modal-xl">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header" style="background-color: #e90000;">
                <h4 class="modal-title colorWhite">@Model.PdfName</h4>
                <button type="button" class="close" data-dismiss="modal" >&times;</button>
            </div>


            <div class="modal-body">
                @*<form>*@
                
                <div class="row">
                    <div class="col-sm-12 col-xs-12">
                        <div class="card backtt">
                            <div class="">
                                <embed src="@Model.PdfPath" width="100%" height="600px;" />

                            </div>
                        </div>
                    </div>

                </div>
                @*</form>*@

            </div>


            <div class="modal-footer" style="background-color:lightgray;">
                @*<button type="button" class="btn btn-info btn-sm" style="background-color: #e90000; width:138px;" id="btnAdd" onclick="return UploadFilledUpFormNew();">Upload</button>*@
                <button type="button" class="btn btn-info btn-sm" style="background-color: #e90000;" data-dismiss="modal">Close</button>
            </div>
        </div>

    </div>
</div>

